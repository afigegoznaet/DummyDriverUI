#!/bin/sh
set -e
if test "$CONFIGURATION" = "Debug"; then :
  cd /Users/verapostanciuc/workspace/DummyDriverUI/buildX
  /usr/local/bin/JUCE-7.0.6/juceaide pkginfo App /Users/verapostanciuc/workspace/DummyDriverUI/buildX/DriverUI_artefacts/JuceLibraryCode/DriverUI/PkgInfo
fi
if test "$CONFIGURATION" = "Release"; then :
  cd /Users/verapostanciuc/workspace/DummyDriverUI/buildX
  /usr/local/bin/JUCE-7.0.6/juceaide pkginfo App /Users/verapostanciuc/workspace/DummyDriverUI/buildX/DriverUI_artefacts/JuceLibraryCode/DriverUI/PkgInfo
fi
if test "$CONFIGURATION" = "MinSizeRel"; then :
  cd /Users/verapostanciuc/workspace/DummyDriverUI/buildX
  /usr/local/bin/JUCE-7.0.6/juceaide pkginfo App /Users/verapostanciuc/workspace/DummyDriverUI/buildX/DriverUI_artefacts/JuceLibraryCode/DriverUI/PkgInfo
fi
if test "$CONFIGURATION" = "RelWithDebInfo"; then :
  cd /Users/verapostanciuc/workspace/DummyDriverUI/buildX
  /usr/local/bin/JUCE-7.0.6/juceaide pkginfo App /Users/verapostanciuc/workspace/DummyDriverUI/buildX/DriverUI_artefacts/JuceLibraryCode/DriverUI/PkgInfo
fi

